		{
			"name": "Get Checklists",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const responseData = pm.response.json();\r",
							"\r",
							"const checklistWithItemsId = parseInt(pm.environment.get(\"checklistWithItemsId\"));\r",
							"const emptyChecklistId = parseInt(pm.environment.get(\"emptyChecklistId\"));\r",
							"\r",
							"const expectedChecklists = [\r",
							"    {\r",
							"        id: checklistWithItemsId,\r",
							"        title: \"New Checklist with Some Items\",\r",
							"        items: [\r",
							"            { text: \"task 1\", isComplete: false },\r",
							"            { text: \"task 2\", isComplete: false },\r",
							"            { text: \"task 3\", isComplete: false }\r",
							"        ]\r",
							"    },\r",
							"    {\r",
							"        id: emptyChecklistId,\r",
							"        title: \"My Empty Checklist\",\r",
							"        items: []\r",
							"    }\r",
							"];\r",
							"\r",
							"pm.test(\"Status Code Is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"All Expected Checklists Are Present and Have Correct Data\", function () {\r",
							"    expectedChecklists.forEach(expectedChecklist => {\r",
							"        const actualChecklist = responseData.find(checklist => checklist.id === expectedChecklist.id);        \r",
							"        pm.expect(actualChecklist.title).to.eql(expectedChecklist.title);\r",
							"        pm.expect(actualChecklist.items.length).to.eql(expectedChecklist.items.length);\r",
							"        \r",
							"        if (expectedChecklist.items.length > 0) {\r",
							"            expectedChecklist.items.forEach((expectedItem, index) => {\r",
							"                const actualItem = actualChecklist.items[index];\r",
							"                pm.expect(actualItem.text).to.eql(expectedItem.text);\r",
							"                pm.expect(actualItem.isComplete).to.eql(expectedItem.isComplete);\r",
							"            });\r",
							"        }\r",
							"    });\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
