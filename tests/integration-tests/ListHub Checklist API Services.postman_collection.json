		{
			"name": "Create Checklist with Some Items",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const responseData = pm.response.json();\r",
							"\r",
							"const checklistWithItemsId = responseData.id;\r",
							"pm.environment.set(\"checklistWithItemsId\", checklistWithItemsId); \r",
							"\r",
							"const firstItem = responseData.items[0];\r",
							"pm.environment.set(\"firstItemId\", firstItem.id);\r",
							"\r",
							"const expectedChecklist = {\r",
							"    title: \"New Checklist with Some Items\",\r",
							"    items: [\r",
							"        { text: \"task 1\", isComplete: false },\r",
							"        { text: \"task 2\", isComplete: false },\r",
							"        { text: \"task 3\", isComplete: false }\r",
							"    ]\r",
							"};\r",
							"\r",
							"pm.test(\"Status Code Is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"'title' Field Has Expected Value\", function () {\r",
							"    pm.expect(responseData.title).to.eql(\"New Checklist with Some Items\");\r",
							"});\r",
							"\r",
							"pm.test(\"Each Checklist Item Has Correct Text and isComplete Status\", function () {\r",
							"    responseData.items.forEach((item, index) => {\r",
							"        const expectedItem = expectedChecklist.items[index];\r",
							"        pm.expect(item.text).to.eql(expectedItem.text);\r",
							"        pm.expect(item.isComplete).to.eql(expectedItem.isComplete);\r",
							"    });\r",
