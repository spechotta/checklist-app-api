		{
			"name": "Add Items to Checklist (Checklist Exists)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let checklist = pm.response.json();\r",
							"\r",
							"if (checklist.items && checklist.items.length > 0) {\r",
							"    checklist.items.forEach((item, index) => {\r",
							"        pm.environment.set(`itemId${index + 1}`, item.id);\r",
							"    });\r",
							"}\r",
							"\r",
							"pm.test(\"First item has correct initial values\", function () {\r",
							"    pm.expect(checklist.items[0].text).to.eql(\"New Item 1\");\r",
							"    pm.expect(checklist.items[0].isComplete).to.eql(false);\r",
							"});\r",
							"\r",
							"pm.test(\"Second item has correct initial values\", function () {\r",
							"    pm.expect(checklist.items[1].text).to.eql(\"New Item 2\");\r",
							"    pm.expect(checklist.items[1].isComplete).to.eql(false);\r",
							"});\r",
							"\r",
							"pm.test(\"Add Items to Checklist (Checklist Exists)\", function () {\r",
							"        pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
